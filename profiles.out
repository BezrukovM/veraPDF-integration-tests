                <message>The document metadata stream doesn't contains PDF/A Identification Schema.</message>
                <arguments/>
            </error>
            <references/>
        </rule>
        <rule object="CosString">
            <id specification="ISO_19005_1" clause="6.1.6" testNumber="1"/>
