<!DOCTYPE html>
<html lang="en-GB">
<head>
  <meta charset="UTF-8">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <meta name="description" content="veraPDF test results.">
  <meta name="author" content="veraPDF Consortium">
  <meta http-equiv="X-UA-Compatible" content="IE=edge">

  <title>veraPDF dest data</title>
  <link href="http://verapdf.org/wp-content/themes/verapdf/includes/img/favicon.ico" rel="icon" type="image/x-icon" />
  <link href="http://software.openpreservation.org/css/vera.integration.css" rel="external" type="text/css" media="screen" />
</head>
<body>
  <h1>veraPDF Test Results</h1>
  {{#corpusDetails}}
  <div id="corpus">
    <h2>{{name}} Corpus</h2>
    {{description}}
  </div>
  {{/corpusDetails}}
  {{#details}}
  <div id="dependencies">
    <h3>Tested:{{dateCreated}}</h3>
    <h3>Dependencies</h3>
    {{#dependencies}}
      <div id="{{id}}">
        <p>Artifact: {{id}}</p>
        <p>Version: {{version}}</p>
        <p>Built: {{buildDate}}</p>
      </div>
    {{/dependencies}}
  </div>
  {{/details}}
  {{#validationProfile}}
  <div id="profile">
    <h2>Validation Profile</h2>
    {{#details}}
    {{name}}
    {{/details}}
  </div>
  {{/validationProfile}}
  <div id="results">
  <table>
  {{#results}}
    <tr>
    {{#result}}
      <td class="{{testType}}">{{corpusItemName}}</td>
    {{/result}}
    </tr>
  {{/results}}
  </table>
  </div>
</body>
</html>
